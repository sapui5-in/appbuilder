<mvc:View controllerName="sapui5in.appbuilder.controller.Home"
	xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core"
	displayBlock="true" xmlns="sap.m">
	<core:Fragment
		fragmentName="sapui5in.appbuilder.modules.branding.fragments.HeaderToolbar"
		type="XML" />
	<TileContainer>
		<tiles>
			<StandardTile title="App Designer"
				press="onPressAppDesigner" />
			<StandardTile title="Custom Control"
				press="onPressCustomControl" visible="false" />
		</tiles>
	</TileContainer>
</mvc:View>